{
    "name": "sweelix/yii2-mailjet",
    "description": "PHP 5.6+ MailJet integration for the Yii framework",
    "keywords": ["yii2", "mailjet", "mail", "email", "mailer"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Philippe Gaultier",
            "email": "pgaultier@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": ">=2.0.4",
        "mailjet/mailjet-apiv3-php": "~1.2",
        "php" : ">=5.6.0"
    },
    "require-dev" : {
        "php" : ">=5.6.0",
        "codeception/codeception": "*"
    },
    "autoload": {
        "psr-4": { "sweelix\\mailjet\\": "src" }
    },
    "extra": {
        "branch-alias": {
            "dev-devel": "1.0.x-dev"
        }
    }
}